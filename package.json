{
  "name": "mongo-admin-server",
  "version": "1.0.2",
  "private": false,
  "description": "Mongo database manager",
  "main": ".next/standalone/server.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "mongo-admin": "node server.js",
    "clean": "rm -rf .cache .tmp .next .swc dist junit.xml junit-ut.xml build tsconfig.tsbuildinfo",
    "test": "npm run test:jest:ci",
