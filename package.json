    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "axios": "^1.4.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "fs": "^0.0.1-security",
    "jose": "^4.14.6",
    "json2csv": "^6.0.0-alpha.2",
    "mongodb": "^5.7.0",
    "mongodb-schema": "^11.2.2",
    "next": "13.4.9",
    "next-intl": "^2.20.0",
